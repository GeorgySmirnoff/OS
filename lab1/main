
#!/bin/bash

source calc
source search
source reverse
source strlen
source _exit
source help
source interactive
source log

case  "$1" in
"calc")
if [[ "$#" -eq 4 ]]
then 
calc "$2" "$3" "$4"
else 
echo "Only 4 args" >&2
exit 5
fi;;
"search")
search "$2" "$3";;
"reverse")
reverse "$2" "$3";;
"strlen")
strlen "$2";;
"_exit")
_exit "$2";;
"help")
help;;
"interactive")
interactive;;
"log")
log;;
esac
